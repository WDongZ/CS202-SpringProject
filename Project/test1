#74（样例里确认键），78（结束样例），7c（确认选择样例）按键，拨码开关70（16位开关），七段数码管64，LED：60(a),62(b)
.data

.text
	addi t1,zero,1
Loop：
	addi s3,zero,0
	lw s4,0xfffffc70(x0)#拨码开关
	lw s5,0xfffffc7c(x0)#s5 确认后开始进行test
	bne s5,t1,Loop
	beq s4,s3,test000
	addi s3,s3,1
	beq s4,s3,test001
	addi s3,s3,1
	beq s4,s3,test010
	addi s3,s3,1
	beq s4,s3,test011
	addi s3,s3,1
	beq s4,s3,test100
	addi s3,s3,1
	beq s4,s3,test101
	addi s3,s3,1
	beq s4,s3,test110
	addi s3,s3,1
	beq s4,s3,test111
	jal exit

test000:
	addi t1,zero,1
	
inputA000:
	lw s1,0xfffffc74(x0)
	lw s2,0xfffffc70(x0)
	sw s2,0xfffffc60(x0)
	bne s1,t1,inputA000
	
inputB000:
	lw s1,0xfffffc78(x0)
	lw s3,0xfffffc70(x0)
	sw s3,0xfffffc62(x0)
	bne s1,t1,inputB000
	jal Loop
	
test001:  
	addi t1,zero,1
	
inputA001:
	lw s1,0xfffffc74(x0)
	lb s2,0xfffffc70(x0)
	bne s1,t1,inputA001
	sw s2,0xfffffc64(x0)
	sw s2,0(x0)
	jal Loop
	
test010:
	addi t1,zero,1
	
inputB010:
	lw s1,0xfffffc74(x0)
	lbu s2,0xfffffc70(x0)
	bne s1,t1,inputB010
	sw s2,0xfffffc64(x0)
	sw s2,4(x0)
	jal Loop
	
test011:
	lw t2,0(x0)#t2加载a
	lw t3,4(x0)#t3加载b
	beq t2,t3,light011
	sw zero,0xfffffc60(a0)#熄灭LED
	jal Loop
light011:
	sw t1,0xfffffc60(a0)#点亮LED
	jal Loop
	
test100:
	lw t2,0(x0)#t2加载a
	lw t3,4(x0)#t3加载b
	blt t2,t3,light100
	sw zero,0xfffffc60(a0)#熄灭LED
	jal Loop
light100:
	sw t1,0xfffffc60(a0)#点亮LED
	jal Loop
	
test101:
	lw t2,0(x0)#t2加载a
	lw t3,4(x0)#t3加载b
	bge t2,t3,light101
	sw zero,0xfffffc60(a0)#熄灭LED
	jal Loop
light101:
	sw t1,0xfffffc60(a0)#点亮LED
	jal Loop
	
test110:
	lw t2,0(x0)#t2加载a
	lw t3,4(x0)#t3加载b
	bltu t2,t3,light110
	sw zero,0xfffffc60(a0)#熄灭LED
	jal Loop
light110:
	sw t1,0xfffffc60(a0)#点亮LED
	jal Loop
	
test111:
	lw t2,0(x0)#t2加载a
	lw t3,4(x0)#t3加载b
	bgeu t2,t3,light111
	sw zero,0xfffffc60(a0)#熄灭LED
	jal Loop
light111:
	sw t1,0xfffffc60(a0)#点亮LED
	jal Loop	
	
exit:
	
	
	
