.data
.text
#60(a),62(b)是输出LED,70是读入拨码开关,78是编码开关,64是七段数码管,74是确认按键
Loop:
addi a3,zero,0xFFFFFC78#a3是编号地址
lw s4,0(a3)
beq a3,s2,test000
beq s4,s2,test001
beq s4,s2,test010
beq s4,s2,test011
beq s4,s2,test100
beq s4,s2,test101
beq s4,s2,test110
beq s4,s2,test111

 #test3'b000
test000:
 li t1,1
addi s0,zero,0xFFFFFC74#s0按键地址
lb s1,0(s0)#s1 确认键 确认后输入
inputA:
lb s1,0(s0)
bne s1,t1,inputA
addi t0,zero,0xFFFFFC70
addi a0,zero,0xFFFFFC60
inputB:
lb s2,0(t0)
sb s2,0(a0)#输出a
addi s0,zero,0xFFFFFC74#s0确认键地址
lb s1,0(s0)#s1确认键
#inputB:
lb s1,0(s0)
bne s1,t1,inputB
addi t1,zero,0xFFFFFC70
addi a0,zero,0xFFFFFC62
lb s3,0(t1)
sb s3,0(a0)#输出b
jal Loop

# test3'b001 
test001:  
li t1,1
addi s0,zero,0xFFFFFC74
lb s1,0(s0)
addi t0,zero,0xFFFFFC70
addi a0,zero,0xFFFFFC64
inputA001:
lb s1,0(s0)
bne s1,t1,inputA001
lbu t2,0(t0)#a存入t2
addi s0,zero,0xFFFFFC74
lb s1,0(s0)#s1确认键
jal Loop

#test3'b010
test010:
li t1,1
addi s0,zero,0xFFFFFC74#s0确认键地址
lb s1,0(s0)#s1确认键
inputB010:
lb s1,0(s0)
bne s1,t1,inputB010
addi t0,zero,0xFFFFFC70
addi a0,zero,0xFFFFFC64
lbu t3,0(t0)#b存入t3
addi s0,zero,0xFFFFFC74
lb s1,0(s0)#s1确认键
jal Loop

#test3'b011
test011:
bne t2,t3,exit011
addi a0,zero,0xFFFFFC60
lb s2,0(t0)
sb s2,0(a0)#点亮LED
exit011:
jal Loop

#test3'b100
test100:
bne t2,t3,exit100
addi a0,zero,0xFFFFFC60
lb s2,0(t0)
sb s2,0(a0)#点亮LED
exit100:
jal Loop

#test3'b101
test101:
bge t2,t3,exit101
addi a0,zero,0xFFFFFC60
lb s2,0(t0)
sb s2,0(a0)#点亮LED
exit101:
jal Loop

#test3'b110
test110:
bgeu t2,t3,exit110
addi a0,zero,0xFFFFFC60
lb s2,0(t0)
sb s2,0(a0)#点亮LED
exit110:
jal Loop

#test3'b111
test111:
bltu t2,t3,exit111
addi a0,zero,0xFFFFFC60
lb s2,0(t0)
sb s2,0(a0)#点亮LED
exit111:
jal Exit

Exit:

